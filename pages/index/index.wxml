<view class="container">
  <swiper indicator-dots='true' autoplay='true' duration='500' interval="2000" circular="true" style='width:100%;height:250px;'>
      <swiper-item wx:for='{{pics}}' style='width:100%;height:90%;'>
        <image src='{{item}}' style='width:100%;height:100%;'></image>
      </swiper-item>
  </swiper>
</view>
